<%= stylesheet_link_tag "post" %>

<div id="post-container">
    <%= link_to "New Blog", new_post_path, id: "new-post"%>

  <div id="posts">
    <% @posts.each do |post| %>
      <div class="post">
        <%= render post %>
        <div style="text-align: right; padding: 0 110px 30px 0;">
          <%= link_to "View More", post, class: "view-more" %>
        </div>
      </div>
    <% end %>
  </div>
</div>